    "bootbuffer": "^0.3.0",
    "bson": "^4.1.0",
    "jest": "^26.4.2",
    "json5": "^2.2.2",
    "node-ts-scripts": "^0.13.1",
    "react": "^16.13.1",
    "react-ace": "^9.1.3",
    "react-dom": "^16.13.1",
    "react-feather": "^2.0.8",
    "react-resizable": "^1.10.1",
    "rippledb": "^1.0.0",
    "semver": "^7.3.2",
    "uuid": "^8.0.0",
    "varint": "^5.0.0"
  },
  "importmap": {
